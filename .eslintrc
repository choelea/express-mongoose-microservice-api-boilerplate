{
  "env": {
    "node": true,
    "jasmine": true
  },
  "plugins": ["jasmine"],
  "extends": ["airbnb/base","plugin:jasmine/recommended"],
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "semi": ["error", "never"],
    "max-len": [2, {"code": 180, "tabWidth": 4, "ignoreUrls": true}],
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
